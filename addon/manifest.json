{
	"manifest_version": 2,
	"name": "rotom-we",
	"version": "0.1.0",
	"author": "XG.Ley",
	"description": "嘻嘻哈哈发表情。",
	"permissions": [
		"<all_urls>",
		"storage",
		"clipboardWrite"
	],
	"icons": {
		"48": "static/icon/icon48.png"
	},
	"options_ui": {
